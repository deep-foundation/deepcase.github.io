{
  "name": "deep-foundation",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "gitpod-export": "export PORT=3007;",
    "local-export": "export PORT=3007;",
    "dev": "next dev -p $PORT",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "gh-pages": "npm run build; rm -rf ./docs; npm run export -- -o docs; touch ./docs/.nojekyll",
    "gitpod": "npm run gitpod-export; export $(grep -v '^#' .env | xargs); npm run dev",
    "local": "npm run local-export; export $(grep -v '^#' .env | xargs); npm run dev"
  },
  "pre-commit": [
    "gh-pages"
  ],
  "dependencies": {
    "@capacitor/core": "^2.4.7",
    "@deepcase/store": "^0.0.32",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@mdx-js/loader": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@next/mdx": "^10.2.0",
    "@use-it/interval": "^1.0.0",
    "next": "^10.0.7",
    "normalize.css": "^8.0.1",
    "pre-commit": "^1.2.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-spring": "^9.1.2"
  },
  "devDependencies": {
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.2",
    "typescript": "^4.2.4"
  },
  "engines": {
    "node": "^14.15.0"
  }
}
